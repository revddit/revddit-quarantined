{
    "name": "revddit quarantined",
    "short_name": "revddit quar",
    "description": "Allows quarantined content to be viewed on revddit.",
    "version": "0.0.0.2",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://*.reddit.com/*",
        "https://revddit.com/*",
        "https://*.reveddit.com/*"
    ],
    "icons": { "128": "icons/128.png",
               "48": "icons/48.png",
               "16": "icons/16.png"
    },
    "content_scripts": [
        {
            "matches": ["https://revddit.com/*", "https://*.reveddit.com/*"],
            "css": ["content.css"],
            "js": []
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "manifest_version": 2
}
